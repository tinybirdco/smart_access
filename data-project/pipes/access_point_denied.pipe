NODE access_point_denied_0
SQL >

    SELECT
      customer_id,
      installation_id,
      access_point_id,
      datetime_window,
      countMerge(cnt_failure) / (countMerge(cnt_success) + countMerge(cnt_failure)) * 100 as access_failure_pct
    FROM access_points_5min_window_mv
    where datetime_window > subtractMinutes(now(), 6)
    group by customer_id, installation_id, access_point_id, datetime_window
    order by access_failure_pct desc
    limit 10


